<UserControl x:Class="WpfProjectTemplate.View.MainWindowContent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
			 xmlns:view="clr-namespace:WpfProjectTemplate.View"
			 xmlns:viewmodel="clr-namespace:WpfProjectTemplate.ViewModel"
			 xmlns:controls="clr-namespace:CommonUI.Controls;assembly=CommonUI"
			 SnapsToDevicePixels="True"
			 FontFamily="Segoe UI Symbol"
             d:DesignHeight="325" d:DesignWidth="525">

    <UserControl.DataContext>
		<viewmodel:MainWindowContentViewModel />
	</UserControl.DataContext>
	
	<!-- Put all stuff in here -->
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto" />
			<RowDefinition Height="*" />			
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="Auto" />
			<ColumnDefinition Width="*" />
		</Grid.ColumnDefinitions>
		
		
	</Grid>
	
	
</UserControl>
